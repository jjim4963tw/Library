<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jjim4963tw.library">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/auto_backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".layout.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".layout.media.StorageAndMediaActivity" />
        <activity android:name=".layout.media.VideoPlayerActivity" />
        <activity android:name="com.jjim4963tw.library.layout.library.jetpack.JetPackMainActivity" />
        <activity android:name="com.jjim4963tw.library.layout.library.jetpack.CameraXActivity" />
        <activity android:name="com.jjim4963tw.library.layout.library.jetpack.FingerBiometricActivity" />
        <activity android:name="com.jjim4963tw.library.layout.library.jetpack.DataStoreActivity" />
        <activity android:name="com.jjim4963tw.library.layout.library.jetpack.LifeCycleActivity" />
        <activity android:name="com.jjim4963tw.library.layout.library.jetpack.LiveDataAndDataBindingActivity" />
        <activity android:name="com.jjim4963tw.library.layout.library.jetpack.ThreadActivity" />
        <activity android:name=".layout.library.RxJavaActivity" />
        <activity android:name=".layout.library.RetrofitActivity" />
        <activity android:name=".layout.library.DaggerActivity" />

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="${applicationId}.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_provider_path" />
        </provider>
    </application>

</manifest>